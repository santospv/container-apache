pipeline{
    agent any
    
    stages {
        stage ('Initial'){
            steps{
                echo 'Iniciando a Pipeline'
            }
        }
        stage ('Build Image'){
            steps{
                echo 'Buildando a Imagem'
                script{
                    dockerapp = docker.build("santospv08/app-apache-docker", ' .')
                }
            }
        }
 
    }
}